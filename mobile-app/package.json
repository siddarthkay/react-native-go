{
  "name": "rn-golang",
  "version": "1.0.0",
  "main": "index.ts",
  "packageManager": "yarn@4.10.3",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "android:build": "./build-android.sh",
    "ios": "expo run:ios",
    "ios:build": "./build-ios.sh",
